<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>CritSpace</display-name>
 
    <!-- VISUAL PROPERTY SERVELETS -->
    <servlet> <servlet-name>VPropTypes</servlet-name>      <servlet-class>org.idch.vprops.servlets.PropertyTypesServlet</servlet-class> </servlet>
    <servlet> <servlet-name>VPropGroups</servlet-name>     <servlet-class>org.idch.vprops.servlets.PropertyGroupsServlet</servlet-class> </servlet>
    <servlet> <servlet-name>VPropProperty</servlet-name>   <servlet-class>org.idch.vprops.servlets.PropertyServlet</servlet-class> </servlet>

    <servlet-mapping> <servlet-name>VPropTypes</servlet-name>        <url-pattern>/vprops/types</url-pattern> </servlet-mapping>
    <servlet-mapping> <servlet-name>VPropGroups</servlet-name>       <url-pattern>/vprops/groups</url-pattern> </servlet-mapping>
    <servlet-mapping> <servlet-name>VPropProperty</servlet-name>     <url-pattern>/vprops/properties</url-pattern> </servlet-mapping>
    
    <!-- CRITSPACE SERVELETS -->
    <servlet> <servlet-name>Workspaces</servlet-name>      <servlet-class>org.idch.critspace.servlets.WorkspaceServlet</servlet-class> </servlet>
    <servlet> <servlet-name>Panels</servlet-name>          <servlet-class>org.idch.critspace.servlets.PanelsServlet</servlet-class> </servlet>
    
    <servlet-mapping> <servlet-name>Workspaces</servlet-name> <url-pattern>/workspaces</url-pattern> </servlet-mapping>
    <servlet-mapping> <servlet-name>Panels</servlet-name>     <url-pattern>/panels</url-pattern> </servlet-mapping>
    
    
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>

